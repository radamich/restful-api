{
    "name": "movisio/restful-api",
    "description": "Nette REST api basics",
    "authors": [
        {
            "name": "Movisio s.r.o."
        }
    ],
    "type": "library",
    "replace": {
        "tprochazka/restful":"*"
    },
    "require-dev": {
        "nette/tester": "^2.4.0",
        "movisio/nais-cs": "2.*",
        "mockery/mockery": "^1.5",
        "nette/application": "^3.1",
        "nette/di": "^3.0"
    },
    "require": {
        "php": "^8.1",
        "nette/utils": "^3.2",
        "nette/schema": "^1.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Drahak\\Restful\\": "src/compatibility/",
            "Movisio\\RestfulApi\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "movisio/nais-cs": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
